class Solution {
    public int numDecodings(String s) {
        if (s == null || s.length() == 0 || s.equals("0")) {
            return 0;
        }

        int[] ans = new int[s.length() + 1];
        ans[0] = 1;
        if (s.charAt(0) != '0') {
            ans[1] = 1;
        }

        for (int i = 2; i <= s.length(); i++) {
            int num1 = s.charAt(i - 2) - '0';
            int num2 = s.charAt(i - 1) - '0';
            int temp = 10 * num1 + num2;

            if (temp >= 10 && temp <= 26) {
                ans[i] += ans[i - 2];
            }

            if (num2 != 0) {
                ans[i] += ans[i - 1];
            }
        }

        return ans[s.length()];
    }
}

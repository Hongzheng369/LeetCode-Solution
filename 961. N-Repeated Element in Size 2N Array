class Solution {
    public int repeatedNTimes(int[] A) {
        HashSet<Integer> appear = new HashSet<>();
        int targetAppear = A.length / 2;
        
        for (int ele : A) {
            if (appear.contains(ele)) {
                return ele;
            }
            
            appear.add(ele);
        }
        
        return 0;
    }
}

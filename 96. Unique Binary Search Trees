class Solution {
    public int numTrees(int n) {
        if (n == 0 || n == 1) {
            return 1;
        }
        int[] answers = new int[n + 1];
        answers[0] = 1;
        answers[1] = 1;
        for (int i = 2; i <= n; i++) {
            for (int j = 0; j < i; j++) {
                answers[i] += answers[j] * answers[i - 1 - j];
            }
        }
        System.out.println(Arrays.toString(answers));
        return answers[n];
    }
}
